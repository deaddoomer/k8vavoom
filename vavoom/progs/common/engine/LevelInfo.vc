		ofloor = GetPlanePointZ(&other->floor, *check->v1);
		if (ofloor > floor)
		{
			floor = ofloor;
			spot = *check->v1;
		}
		ofloor = GetPlanePointZ(&other->floor, *check->v2);
		if (ofloor > floor)
