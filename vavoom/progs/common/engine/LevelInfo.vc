//==========================================================================
//
//	CreateTIDList
//
//==========================================================================

void CreateTIDList()
{
	int i;
	Entity mobj;

	TIDList.Num = 0;
	TIDMobj.Num = 0;
	i = 0;
	foreach AllThinkers(Entity, mobj)
	{
		if (mobj.TID != 0)
		{
			//	Add to list
			TIDList[i] = mobj.TID;
			TIDMobj[i++] = mobj;
		}
	}
}

