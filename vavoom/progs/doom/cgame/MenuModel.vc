class MenuModel : MenuObject;

TVec Origin;
float Angle;
void* Model;
int Frame;
string Skin;

void InitWindow()
{
	::InitWindow();
	Disable();
}

void OnDraw()
{
	R_DrawModelFrame(Origin, Angle, Model, Frame, Skin);
}

defaultproperties
{
	Skin = "";
}
