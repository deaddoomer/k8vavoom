//**************************************************************************
//
//	Puff
//
//**************************************************************************

class Puff:Effect
{

__states__
{
	S_PUFF1('PUFF', 0 | FF_FULLBRIGHT, 4.0 / 35.0, S_PUFF2) { }
	S_PUFF2('PUFF', 1, 4.0 / 35.0, S_PUFF3) { }
	S_PUFF3('PUFF', 2, 4.0 / 35.0, S_PUFF4) { }
	S_PUFF4('PUFF', 3, 4.0 / 35.0, S_NULL) { }
}

defaultproperties
{
	Translucency = 33;
	IdleState = S_PUFF1;
}

}

