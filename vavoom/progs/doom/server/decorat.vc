//**************************************************************************
//
//	Barrel
//
//**************************************************************************

class Barrel:Decoration
	__mobjinfo__(2035)
{

__states__
{
	S_BAR1('BAR1', 0, 'objects/barrel.md2', 0, 6.0 / 35.0, S_BAR2) { }
	S_BAR2('BAR1', 1, 'objects/barrel.md2', 1, 6.0 / 35.0, S_BAR1) { }
	S_BEXP('BEXP', 0 | FF_FULLBRIGHT, 5.0 / 35.0, S_BEXP2) { }
	S_BEXP2('BEXP', 1 | FF_FULLBRIGHT, 5.0 / 35.0, S_BEXP3) { A_Scream(); }
	S_BEXP3('BEXP', 2 | FF_FULLBRIGHT, 5.0 / 35.0, S_BEXP4) { }
	S_BEXP4('BEXP', 3 | FF_FULLBRIGHT, 10.0 / 35.0, S_BEXP5) { A_Explode(); }
	S_BEXP5('BEXP', 4 | FF_FULLBRIGHT, 10.0 / 35.0, S_NULL) { }
}

defaultproperties
{
	Health = 20;
	Radius = 10.0;
	Height = 42.0;
	bSolid = true;
	bShootable = true;
	bNoBlood = true;
	IdleState = S_BAR1;
	DeathState = S_BEXP;
	DeathSound = 'BarrelExplode';
}

}

