//==========================================================================
//
//	A_KeenDie
//
//	DOOM II special, map 32. Uses special tag 666.
//
//==========================================================================

void A_KeenDie(mobj_t *mo)
{
	int			args[5];

	A_Fall(mo);
    
    // scan the remaining thinkers to see if all Keens are dead
	if (P_FindOther(mo))
    {
    	return;
	}
    args[0] = 666;
	args[1] = 16;
	args[2] = 150;
	args[3] = 0;
    EV_DoDoor(NULL, &args[0], doorOpen);
}

#else

void A_KeenDie(mobj_t *mo) { NotDoom2(); }

#endif
