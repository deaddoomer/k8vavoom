//==========================================================================
//
//  Actor::A_KeenDie
//
//  DOOM II special, map 32. Uses special tag 666.
//
//==========================================================================

void Actor::A_KeenDie(void)
{
	int args[5];

	A_Fall();

	// scan the remaining thinkers to see if all Keens are dead
	if (P_FindOther(this))
	{
		return;
	}
	args[0] = 666;
	args[1] = 16;
	args[2] = 150;
	args[3] = 0;
	EV_DoDoor(NULL, &args[0], doorOpen);
}

#else

void Actor::A_KeenDie(void)
{
	NotDoom2();
}

#endif

