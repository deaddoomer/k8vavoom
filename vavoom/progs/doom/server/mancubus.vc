//**************************************************************************
//
//	MancubusMissile
//
//**************************************************************************

class MancubusMissile:Projectile
{

__states__
{
	S_FATSHOT1('MANF', 0 | FF_FULLBRIGHT, 'missiles/mancmisl.md2', 0, 4.0 / 35.0, S_FATSHOT2) { }
