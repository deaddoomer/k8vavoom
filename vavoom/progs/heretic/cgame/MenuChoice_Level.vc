class MenuChoice_Level : MenuChoiceEnum;

void InitMapList(int epi)
{
	int i;

	for (i = 0; i < 9; i++)
	{
		EnumNames[i] = P_GetMapName(epi * 9 + i);
	}
	MaxValue = 9;
	Value = 0;
}

void InitWindow()
{
	::InitWindow();
	InitMapList(0);
}

defaultproperties
{
	Text = "Level";
}
