class ItemKeyBlue : Key
	__mobjinfo__(79)
	__scriptid__(85);

void Tick(float FrameTime)
{
	Angles.yaw = AngleMod360(Angles.yaw + FrameTime * 180.0);
	::Tick(FrameTime);
}

