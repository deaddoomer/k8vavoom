class KeyGizmoYellow : Decoration
	__mobjinfo__(96);

//==========================================================================
//
//  A_InitKeyGizmo
//
//==========================================================================

final void A_InitKeyGizmo()
{
	Spawn(KeyGizmoFloatYellow, Origin + vector(0.0, 0.0, 60.0));
}

__states__
{
	S_KEYGIZMO1('KGZ1', 0, 1.0 / 35.0, S_KEYGIZMO2) = none
	S_KEYGIZMO2('KGZ1', 0, 1.0 / 35.0, S_KEYGIZMO3) = A_InitKeyGizmo
	S_KEYGIZMO3('KGZ1', 0, -1.0, none) = none
}

defaultproperties
{
	Radius = 16.0;
	Height = 50.0;
	bSolid = true;
	bStaticLight = true;
	LightColour = RGB(255, 255, 0);
	LightOffset = vector(0.0, 0.0, 68.0);
	IdleState = S_KEYGIZMO1;
}
