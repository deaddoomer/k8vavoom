class KeyGizmoFloat : Decoration;

__states__
{
	S_KGZ_START('KGZB', 0, 'objects/orbblue.md2', 0, 1.0 / 35.0, S_KGZ_START) { }
	S_KGZ_BLUEFLOAT1('KGZB', 0 | FF_FULLBRIGHT, 'objects/orbblue.md2', 0, -1.0, none) { }
	S_KGZ_GREENFLOAT1('KGZG', 0 | FF_FULLBRIGHT, 'objects/orbgreen.md2', 0, -1.0, none) { }
	S_KGZ_YELLOWFLOAT1('KGZY', 0 | FF_FULLBRIGHT, 'objects/orbyel.md2', 0, -1.0, none) { }
}

defaultproperties
{
	Radius = 16.0;
	Height = 16.0;
	bSolid = true;
	bNoGravity = true;
	IdleState = S_KGZ_START;
}

