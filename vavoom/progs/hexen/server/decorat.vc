	__mobjinfo__(8064)
{

//===========================================================================
//
//  A_SoAExplode
//
//  Suit of Armor Explode
//
//===========================================================================

void A_SoAExplode(void)
{
	Actor A;
	int i;

	for (i = 0; i < 10; i++)
	{
		A = Spawn(ZArmorChunk, Origin + vector((Random() - 0.5) * 16.0,
			(Random() - 0.5) * 16.0, Random() * Height));
		A.SetState(A.IdleState + i);
		if (A)
		{
			A.Velocity.x = (Random() - Random()) * 4.0 * 35.0;
			A.Velocity.y = (Random() - Random()) * 4.0 * 35.0;
			A.Velocity.z = (Random() * 8.0 + 5.0) * 35.0;
		}
	}
	if (Args[0])
	{
		// Spawn an item
		Spawn(TranslateThingType[Args[0]], Origin);
	}
	A.PlaySound('SuitofArmorBreak', CHAN_VOICE);
	Remove();
}

__states__
{
	S_ZSUITOFARMOR('SUIT', 0, -1.0, S_NULL) { }
	S_ZSUITOFARMOR_X1('SUIT', 0, 1.0 / 35.0, S_NULL) { A_SoAExplode(); }
}

defaultproperties
{
