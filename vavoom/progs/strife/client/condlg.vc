//==========================================================================
//
//	StartSpeech
//
//==========================================================================

void StartSpeech(int SpeechNum)
{
	ConDialog Dlg;

	Dlg = ConDialog(SpawnMenu(ConDialog));
	Dlg.SetSpeech(SpeechNum);
	PushMenuScreen(Dlg);
}

