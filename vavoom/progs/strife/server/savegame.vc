//==========================================================================
//
//  ArchivePlayer
//
//==========================================================================

void ArchivePlayer(player_t * player)
{
}

//==========================================================================
//
//  UnarchivePlayer
//
//==========================================================================

void UnarchivePlayer(player_t * player)
{
	player->attacker = NULL;
}

//==========================================================================
//
//  mobj_t::Archive
//
//==========================================================================

void mobj_t::Archive(void)
{
	MobjToNum(&target);
}

//==========================================================================
//
//  mobj_t::Unarchive
//
//==========================================================================

void mobj_t::Unarchive(void)
{
	NumToMobj(&target);
	SightSound = UnarchiveName(SightSound);
	ActiveSound = UnarchiveName(ActiveSound);
	AttackSound = UnarchiveName(AttackSound);
	PainSound = UnarchiveName(PainSound);
	DeathSound = UnarchiveName(DeathSound);
}

