class WaterfallSplash : Decoration
	__mobjinfo__(104);

states
{
Spawn:
	SPLH ABC 4
	SPLH D 8
	SPLH EFG 4
	SPLH H 4 A_ActiveSound
	Loop
}

defaultproperties
{
	bNoBlockmap = true;
	ActiveSound = 'world/waterfall';
}
