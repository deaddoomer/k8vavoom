class AlnBoublesCeiling:Decoration
	__mobjinfo__(223);

__states__
{
	S_BUBC('BUBC', 0, 4.0 / 35.0, S_BUBC) { A_ActiveSound(); }
	//FIXME
	S_BUBF('BUBF', 0, 4.0 / 35.0, S_BUBF) { A_ActiveSound(); }
}

defaultproperties
{
	Radius = 16.0;
	Height = 72.0;
	bSolid = true;
	bSpawnCeiling = true;
	bNoGravity = true;
	// NOTE There must be S_BUBC
	IdleState = S_BUBF;
	ActiveSound = 'ambient/alien4';
}

