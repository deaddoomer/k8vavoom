class WoodenBarrel : Decoration
	__mobjinfo__(82);

states
{
Spawn:
	BARW A -1
	Stop
Death:
	BARW B 2 A_Scream
	BARW C 2
	BARW D 2 A_Fall
	BARW EFG 2
	BARW H -1
	Stop
}

defaultproperties
{
	Health = 10;
	Radius = 10.0;
	Height = 32.0;
	bSolid = true;
	bShootable = true;
	bInCombat = true;
	bNoBlood = true;
	DeathSound = 'woodenbarrel/death';
}
