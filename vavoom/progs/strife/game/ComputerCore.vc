class ComputerCore : Decoration
	__mobjinfo__(182);

__states__
{
	S_COMP_CORE_1('SECR', 0 | FF_FULLBRIGHT, 4.0 / 35.0, S_COMP_CORE_2) { }
	S_COMP_CORE_2('SECR', 1 | FF_FULLBRIGHT, 4.0 / 35.0, S_COMP_CORE_3) { }
	S_COMP_CORE_3('SECR', 2 | FF_FULLBRIGHT, 4.0 / 35.0, S_COMP_CORE_4) { }
	S_COMP_CORE_4('SECR', 3 | FF_FULLBRIGHT, 4.0 / 35.0, S_COMP_CORE_1) { }
	S_COMP_CORE_X1('SECR', 4 | FF_FULLBRIGHT, 5.0 / 35.0, S_COMP_CORE_X2) { A_SpawnPuff(); }
	S_COMP_CORE_X2('SECR', 5 | FF_FULLBRIGHT, 5.0 / 35.0, S_COMP_CORE_X3) { A_Fall(); }
	S_COMP_CORE_X3('SECR', 6 | FF_FULLBRIGHT, 5.0 / 35.0, S_COMP_CORE_X4) { A_QuestObjectDeath(); }
	S_COMP_CORE_X4('SECR', 7 | FF_FULLBRIGHT, 5.0 / 35.0, S_COMP_CORE_X5) { A_XDying(); }
	S_COMP_CORE_X5('SECR', 8 | FF_FULLBRIGHT, 5.0 / 35.0, S_COMP_CORE_X6) { A_SpawnPuff(); }
	S_COMP_CORE_X6('SECR', 9, 5.0 / 35.0, S_COMP_CORE_X7) { }
	S_COMP_CORE_X7('SECR', 10, 5.0 / 35.0, S_COMP_CORE_X8) { A_SpawnPuff(); }
	S_COMP_CORE_X8('SECR', 11, 5.0 / 35.0, S_COMP_CORE_X9) { }
	S_COMP_CORE_X9('SECR', 12, 5.0 / 35.0, S_COMP_CORE_X10) { A_SpawnPuff(); }
	S_COMP_CORE_X10('SECR', 13, 5.0 / 35.0, S_COMP_CORE_X11) { }
	S_COMP_CORE_X11('SECR', 14, 5.0 / 35.0, S_COMP_CORE_X12) { A_SpawnPuff(); }
	S_COMP_CORE_X12('SECR', 15, -1.0, none) { }
}

defaultproperties
{
	Health = 80;
	Radius = 26.0;
	Height = 128.0;
	Mass = 100000.0;
	bSolid = true;
	bShootable = true;
	bInCombat = true;
	bNoBlood = true;
	IdleState = S_COMP_CORE_1;
	DeathState = S_COMP_CORE_X1;
	DeathSound = 'misc/explosion';
	bGiveQuest = true;
	QuestID = QUEST_BlownUpComputer;
}

