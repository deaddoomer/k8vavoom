class FireFoot:Effect;

__states__
{
	S_FFOT_1('FFOT', 0, 9.0 / 35.0, S_FFOT_2) { }
	S_FFOT_2('FFOT', 1, 9.0 / 35.0, S_FFOT_3) { }
	S_FFOT_3('FFOT', 2, 9.0 / 35.0, S_FFOT_4) { }
	S_FFOT_4('FFOT', 3, 9.0 / 35.0, none) { }
}

defaultproperties
{
	bNoGravity = false;
	bColideWithThings = false;
	bColideWithWorld = false;
	IdleState = S_FFOT_1;
}

