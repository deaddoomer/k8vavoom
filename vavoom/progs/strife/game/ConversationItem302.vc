class ConversationItem302 : Pickup;

//==========================================================================
//
//	HandlePickup
//
//==========================================================================

bool HandlePickup(Actor Toucher)
{
	int Args[5];

	Args[0] = 222;
	Args[1] = 16;
	Args[2] = 0;
	Args[3] = 0;
	Args[4] = 0;
	return ExecuteLineSpecial(LNSPEC_DoorOpen, &Args[0], NULL, 0, Toucher);
}

defaultproperties
{
	ConversationID = 302;
	IdleState = S_TOKENS;
}

