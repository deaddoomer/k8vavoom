class ConversationItem305 : Pickup;

//==========================================================================
//
//	HandlePickup
//
//==========================================================================

bool HandlePickup(Actor Toucher)
{
	return Level.ExecuteActionSpecial(LNSPEC_DoorOpen, 224, 16, 0, 0, 0,
		NULL, 0, Toucher);
}

defaultproperties
{
	ConversationID = 305;
	IdleState = S_TOKENS;
	bDropAutoGive = true;
}

