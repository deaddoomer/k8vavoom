class PoisonArrow : Projectile;

__states__
{
	S_ARROW2('ARWP', 0, 10.0 / 35.0, S_ARROW2) { A_ActiveSound(); }
	S_ARROW2_X('AROW', 0, 1.0 / 35.0, S_NULL) { }
}

defaultproperties
{
	Radius = 10.0;
	Height = 10.0;
	Speed = 1050.0;
	MissileDamage = 500;
	bActivateImpact = true;
	IdleState = S_ARROW2;
	DeathState = S_ARROW2_X;
	SightSound = 'misc/swish';
	ActiveSound = 'misc/swish';
	bPoisonDamage = true;
}

