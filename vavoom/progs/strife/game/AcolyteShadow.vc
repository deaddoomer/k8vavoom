class AcolyteShadow : Acolyte
	__mobjinfo__(58);

states
{
See:
	AGRD A 6 A_BeShadowyFoe
	Goto Super::See+1
Pain:
	AGRD O 0 A_SetShadow
	AGRD O 8 A_Pain
	Goto See
}

defaultproperties
{
	ConversationID = 61;
}
