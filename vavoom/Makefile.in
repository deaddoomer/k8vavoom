top_srcdir=@top_srcdir@
srcdir=@srcdir@

@SET_MAKE@

.PHONY: all clean install

all:
	$(MAKE) -C utils
	$(MAKE) -C source
	$(MAKE) -C progs
	$(MAKE) -C basev

install:
	$(MAKE) install -C utils
	$(MAKE) install -C source
	$(MAKE) install -C progs
	$(MAKE) install -C basev

clean:
	$(MAKE) clean -C utils
	$(MAKE) clean -C source
	$(MAKE) clean -C progs
	$(MAKE) clean -C basev
