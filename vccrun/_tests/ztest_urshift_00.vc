class Main : Object;

void main () {
  writeln(-1>>1, " : ", -1>>>1, " : ", va("%x", -1>>>1));
  int n0 = 0x8000_0000;
  int n1 = n0;
  writeln(n0);
  writeln(n0>>1, " : ", n0>>>1, " : ", va("%x", n0>>1), " : ", va("%x", n0>>>1));
  n0 >>= 1;
  n1 >>>= 1;
  writeln(va("%x", n0), " : ", va("%x", n1));
}
