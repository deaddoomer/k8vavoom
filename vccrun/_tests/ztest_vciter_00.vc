class Main : Object;

struct IterInfo {
  int cur;
}


void testIter_opInit (out IterInfo it, int initval) {
  it.cur = initval;
  writeln("iterator initialized");
}

void testIter_opDone (ref IterInfo it) {
  writeln("iterator deinitialized, cur=", it.cur);
}

bool testIter_opNext (ref IterInfo it, ref int v) {
  if (it.cur > 6) return false;
  v = it.cur;
  ++it.cur;
  return true;
}


void main () {
  foreach (int n; testIter(2)) {
    writeln("n=", n);
  }
}
