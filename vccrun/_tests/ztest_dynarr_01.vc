class Main : Object;

void main () {
  array!string a;
  foreach (auto idx; 0..6) a[idx] = string.fromInt(idx+1);
  writeln("==================="); foreach (auto idx, auto v; a) writeln("[", idx, "] = <", v, ">");
  a.remove(3, 2);
  writeln("==================="); foreach (auto idx, auto v; a) writeln("[", idx, "] = <", v, ">");
  a.remove(3, 1);
  writeln("==================="); foreach (auto idx, auto v; a) writeln("[", idx, "] = <", v, ">");
  a.insert(1, 3);
  writeln("==================="); foreach (auto idx, auto v; a) writeln("[", idx, "] = <", v, ">");
}
