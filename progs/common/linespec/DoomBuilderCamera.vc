class DoomBuilderCamera : Actor
  __mobjinfo__(32000);

//==========================================================================
//
//  BeginPlay
//
//==========================================================================

void BeginPlay()
{
  ::BeginPlay();
  Destroy();
}

defaultproperties
{
  bNoBlockmap = true;
  bNoSector = true;
}
